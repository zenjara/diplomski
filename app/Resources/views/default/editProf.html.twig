{% extends 'base.html.twig' %}
{% block body %}
    <div class="container edit">
        {{ form_start(form) }}
        {{ form_row(form.username) }}
        {{ form_row(form.email) }}
        {{ form_row(form.first_name) }}
        {{ form_row(form.last_name) }}
        {{ form_row(form.date_of_birth) }}
        <button type="submit" class="btn btn-primary">Update</button>
    </div>


    {{ form_end(form) }}
{% endblock body %}